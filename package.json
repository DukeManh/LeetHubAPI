{
  "name": "leethub",
  "version": "1.0.0",
  "description": "Automatically uploads Leetcode submissions to Github",
  "main": "dist/app.js",
  "nodemonConfig": {
    "watch": [
      "src/**/*.ts"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "execMap": {
      "ts": "ts-node"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "echo \"Best of luck\" && tsc && nodemon dist/app.js"
  },
  "author": "Chris Bui",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.8",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "follow-redirects": "^1.13.0",
    "graphql": "^15.4.0",
    "graphql-request": "^3.2.0",
    "morgan": "^1.10.0",
    "readme-md-generator": "^1.0.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
